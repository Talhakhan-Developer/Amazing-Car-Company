    <!-- Book A Service -->
    <div class="container pt-5 mb-5">
      <div
        class="booking-area h-100 d-flex flex-column justify-content-center text-center p-5"
      >
        <h1 class="mb-5">Book For A Service</h1>
        <form method="POST" action="/booking/create">
          <div class="row g-3">
            <div class="col-12 col-sm-6">
              <input
                name="name"
                type="text"
                class="form-control rounded-pill"
                placeholder="Your Name"
                style="height: 55px;"
              />
            </div>
            <div class="col-12 col-sm-6">
              <input
                name="email"
                type="email"
                class="form-control rounded-pill"
                placeholder="Your Email"
                style="height: 55px;"
              />
            </div>
            <div class="col-12 col-sm-6">
              <select
                name="service"
                class="form-select rounded-pill"
                style="height: 55px;"
              >
                <option selected>Select A Service</option>
                <option value="Diagnostic">Diagnostic</option>
                <option value="Batteries">Batteries</option>
                <option value="Breaks">Breaks</option>
                <option value="Engine Repair">Engine Repair</option>
                <option value="Tire Replacement">Tire Replacement</option>
                <option value="Transmission">Transmission</option>
              </select>
            </div>
            <div class="col-12 col-sm-6">
              <input
                name="appointmentDate"
                type="date"
                class="form-control datetimepicker-input rounded-pill"
                placeholder="Service Date"
                data-target="#date1"
                data-toggle="datetimepicker"
                style="height: 55px;"
              />
            </div>
            <div class="col-12">
              <textarea
                name="description"
                class="form-control"
                placeholder="Basic Information"
              ></textarea>
            </div>
            <div class="col-12">
              <button class="btn btn-search w-100 py-3" type="submit">Book Now</button>
            </div>
          </div>
        </form>
      </div>
    </div>
    <!-- Book A Service -->

